require.config
  baseUrl: '/assets'
  paths:
    'toastmessage': 'toastmessage/javascript/jquery.toastmessage'
  shim:
    'space-pen':
      exports: 'View'
      deps: ['jquery']
    'amplify':
      deps: ['jquery']
      exports: 'amplify'
    'toastmessage':
      deps: ['jquery']
    'jquery-ui':
      deps: ['jquery']
    'jstree/jquery.jstree':
      deps: ['jquery']
    'taffy':
      exports: 'TAFFY'
    'underscore':
      exports: '_'
    'path':
      exports: 'Path'

<% if Rails.env.production? %>
require ['psi', 'utils/dummy_logger'], (PSI, Logger)->
<% else %>
require ['psi', 'utils/logger'], (PSI, Logger)->
<% end %>
  window.psi = new PSI logger: new Logger
  $ -> psi.init()
